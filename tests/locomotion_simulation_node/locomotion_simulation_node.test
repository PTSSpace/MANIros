<launch>
  <arg name="rover_parameters_url" default="$(find maniros)/config/mani.yaml" />
  <rosparam command="load" file="$(arg rover_parameters_url)" />

  <node name="locomotion_simulation_node" pkg="maniros" type="locomotion_simulation_node.py" output="screen"/>
  <test test-name="locomotion_simulation_node_test" pkg="maniros" type="locomotion_simulation_node_test.py"/>
</launch>
